  <Description>Orders on Credit Hold with a back ordered line that has no PO</Description>
  <Author>Thomas Van Nuys</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="GartmanReports">
      <DataSourceReference>/GartmanReports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2cfca2ff-4411-48a3-9e4a-f44afdb8a325</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select * from openquery(gsfl2k,'
select h.ohord#,
h.ohcust,
h.ohdate

from oohead h 
left JOIN OOLINE l ON (l.OLCO = h.OHCO 
	AND l.OLLOC = h.OHLOC
	AND l.OLORD# = h.OHORD# 
	AND l.OLREL# = h.OHREL#) 
	
left join poboline p on (l.OLCO = p.pboco
	AND l.OLLOC = p.pboloc
	AND l.OLORD# = p.pboord
	AND l.OLREL# = p.pboREL
	and l.olseq# = p.pboseq) 

/* where h.ohord# in (''557372'',''580559'') */
where OLBO = ''Y''
and h.OHCRHL = ''Y''
and pbpo# is null

group by h.ohord#,
h.ohcust,
h.ohdate

')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OHORD_">
          <DataField>OHORD#</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="OHDATE">
          <DataField>OHDATE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OHCUST">
          <DataField>OHCUST</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
