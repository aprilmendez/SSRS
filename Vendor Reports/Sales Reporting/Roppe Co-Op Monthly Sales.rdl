  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>SELECT	SLCUST AS CustNbr, 
		CMNAME AS CustName, 
		Left(CMADR3,23) AS City, 
		Right(CMADR3,2) AS State, 
		CMZIP AS Zip, 
		SLINV# AS InvoiceNbr, 
		substring(sldate,6,2) +'/'+ right(sldate,2) +'/'+ left(sldate,4) AS InvoiceDate, 
		IMSKEY AS ItemNbr, 
		SLEPRC AS Total$, 
		SLINV# + '-' + SLITEM AS InvoiceItem, 
		SLDATE

from openquery (gsfl2k,'

SELECT	SLCUST, 
		CMNAME, 
		CMADR3, 
		CMZIP, 
		SLINV#, 
		IMSKEY, 
		SLEPRC, 
		SLITEM,
		SLDATE
		
FROM CUSTMAST 
	INNER JOIN SHLINE ON CUSTMAST.CMCUST = SHLINE.SLCUST
	INNER JOIN CUSTBGRP ON (CUSTMAST.CMBILL = CUSTBGRP.CGCUST and custbgrp.cgbgrp = ''ROCOOP'')
	INNER JOIN ITEMMAST ON SHLINE.SLITEM = ITEMMAST.IMITEM

WHERE (year(sldate)=year(current_date - 1 month) and month(sldate)=month(current_date - 1 month))
	AND SHLINE.SLEPRC &lt;&gt; 0 
	AND SHLINE.SLVEND = 10202 
	AND SHLINE.SLPRCD not in (640,12990,775,11370,11371,11372,11373,11374,11375,11365,12551,12960)
	AND SHLINE.SLITEM not in (''ROBROKENCARTON'',''CLAIMVARROP'',''CLAIMROP'',''CLAIMLABORROP'',''SAMPLEFREIGHTROP'')

')

order by InvoiceNbr, ItemNbr</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
