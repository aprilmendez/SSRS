  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select * from openquery(gsfl2k,'

SELECT ITEMDETL.IDITEM AS ItemNbr, 
ITEMMAST.IMDESC AS Description, 
ITEMMAST.IMCOLR, 
ITEMMAST.IMVEND AS VendorNum, 
vmname as Vendor,
ITEMDETL.IDCO AS Company, 
ITEMDETL.IDLOC AS Location, 
/* ITEMDETL.IDKY AS InventoryKey,  */
/* ITEMDETL.IDSERL AS SerialNbr,  */
ITEMDETL.IDDATE AS InventoryDate, 
itemdrop.idrdat as ItemDropDate,
days(current_date) - days(iddate) as Age,
/* ITEMDETL.IDBIN AS BinLoc, */
ITEMDETL.IDQOH AS InventoryQtyOnHand, 
IDQOH-IDQOO AS InventoryQtyAvail, 
/* ITEMMAST.IMDROP,  */
ITEMMAST.IMUM2 as SalesUOM, 
ITEMMAST.IMUM1 as InventoryUOM, 
ITEMMAST.IMFACT as ConversionToSalesFactor, 
ITEMDETL.IDCOST as SalesUnitCost,
immd,
immd2,

case 
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = '' '') then (IdQOH*itemmast.IMFACT)
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = ''0'') then (IdQOH*itemmast.IMFACT)
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = ''D'') then ((IdQOH*itemmast.IMFACT)/itemmast.IMFAC2)
	else 0
end as SalesUOMOnHand,

case 
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = '' '') then (IdQOH*itemmast.IMFACT*idcost)
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = ''0'') then (IdQOH*itemmast.IMFACT*idcost)
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = ''D'') then (((IdQOH*itemmast.IMFACT)/itemmast.IMFAC2)*idcost)
	else 0
end as Value

FROM ITEMDETL 
left JOIN ITEMMAST ON ITEMDETL.IDITEM = ITEMMAST.IMITEM
left join itemdrop on idritm = imitem
left join vendmast on imvend = vmvend

WHERE IDQOH-IDQOO &gt; 0 
AND ITEMMAST.IMDROP=''D''
AND ITEMMAST.IMFCRG &lt;&gt; ''S''

order by ITEMDETL.IDCO,
ITEMDETL.IDLOC 

')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
