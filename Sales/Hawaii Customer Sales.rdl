  <Description>Hawaii customer sales for Greg Szalay</Description>
  <Author>Thomas Van Nuys</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="GartmanReports">
      <DataSourceReference>GartmanReports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>250364cd-8b76-4691-b5cb-73428aa8d63c</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select *
from openquery(gsfl2k,'
select Soldto.cmname as SoldToCustName,
	Soldto.CMcust AS SoldToCustID, 
	Soldto.CMLOC as SoldToMasterLocation,
	Left(Soldto.CMADR3,23) AS SoldToCity,
	Right(Soldto.CMADR3,2) AS SoldToState,

ifnull((select sum(SLEPRC) from  SHLINE L1 left JOIN SHHEAD H1 ON (L1.SLCO = H1.SHCO 
	AND L1.SLLOC = H1.SHLOC 
	AND L1.SLORD# = H1.SHORD# 
	AND L1.SLREL# = H1.SHREL# 
	AND L1.SLINV# = H1.SHINV#)
	where SoldTo.CMCust = H1.SHCUST 
	and shidat &gt;= current_date - 12 months),0) as Last12MonthSales,
 
ifnull((select sum(SLEPRC) from  SHLINE L2 left JOIN SHHEAD H2 ON (L2.SLCO = H2.SHCO 
	AND L2.SLLOC = H2.SHLOC 
	AND L2.SLORD# = H2.SHORD# 
	AND L2.SLREL# = H2.SHREL# 
	AND L2.SLINV# = H2.SHINV#)
	where SoldTo.CMCust = H2.SHCUST
	and shidat &gt;= current_date - 24 months 
	and shidat &lt;= current_date - 12 months),0) as Previous12MonthSales


from custmast soldto 
		
where Right(Soldto.CMADR3,2) in (''HI'')

order by 1,4
')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SOLDTOCUSTNAME">
          <DataField>SOLDTOCUSTNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SOLDTOCUSTID">
          <DataField>SOLDTOCUSTID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SOLDTOMASTERLOCATION">
          <DataField>SOLDTOMASTERLOCATION</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SOLDTOCITY">
          <DataField>SOLDTOCITY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SOLDTOSTATE">
          <DataField>SOLDTOSTATE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LAST12MONTHSALES">
          <DataField>LAST12MONTHSALES</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PREVIOUS12MONTHSALES">
          <DataField>PREVIOUS12MONTHSALES</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=cdec(fields!LAST12MONTHSALES.Value+fields!PREVIOUS12MONTHSALES.Value)</FilterExpression>
          <Operator>NotEqual</Operator>
          <FilterValues>
            <FilterValue DataType="Float">=cdec(0)</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
