  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select imitem as 'SKU/UPC',
cuicit as 'ASIN',
' ' as Title,
'T&amp;A SUPPLY COMPANY' AS 'WAREHOUSE_NAME',
'ABMB' as 'WAREHOUSE_CODE',
cast(QAvail as int) as 'QUANTITY'

from openquery(gsfl2k,'
SELECT IMITEM, 
custitem.cuicit,
sum(ibqoh-ibqoo) as QAvail

FROM ITEMMAST 

JOIN CUSTITEM on (CUIITM = IMITEM and cuicus = ''1005231'')
left JOIN ITEMBAL ON IMITEM = IBITEM

where IMRPT1=42
and ((ibco=2 and ibloc=41) or
	 (ibco=2 and ibloc=35) or
	 (ibco=2 and ibloc=42) or
	 (ibco=1 and ibloc=50) or
	 (ibco=1 and ibloc=52) or
	 (ibco=1 and ibloc=60))
	 
group by IMITEM, 
custitem.cuicit
')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
