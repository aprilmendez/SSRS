  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select * from openquery (gsfl2k,'

select	iditem as ItemNum,
		imdesc as ItemDesc,
		iddate as DateReceived,
		/* idrdat as ReceiptDate, */
		idco as Company,
		idloc as Location,
		idbin as BinLoc,
		idserl as SerialNum,
		imshelflf as ShelfLifeDays,
		/* (imshelflf - 60) as warningdays,*/
		/* (iddate + (imshelflf - 60)days ) as warningdate,*/
		(iddate + (imshelflf)days ) as expirationdate,
	case
		when ((iddate+(imshelflf)days)&lt;current_date)
		then ''Yes'' else ''No''
						end as expiredflag,
	case
		when ibprio=''Y'' then ''Yes''
		else ''No''
						end as locstock,
	
	case
		when imdrop = ''D''
		then ''Yes'' else ''No''
						end as dropped,
	
		idqoh as QtyOnHand,
		idohbl as QtyAvailable,
		/* idcost as UnitCost, */
	case
		WHEN (immd = ''M'' AND immd2 = '' '') 
						THEN CAST(ROUND((idqoh * imfact) * idcost,3) AS decimal (18,2))
					WHEN (immd = ''M'' AND immd2 = ''D'') 
						THEN CAST(ROUND(((idqoh * imfact) / imfac2) * idcost,3) AS decimal (18,2)) 
						end as OnHandValue
		
from itemdetl id
	left join itemmast im on im.imitem = id.iditem
	inner join itemxtra ix on (ix.imxitm = id.iditem and ix.imshelflf &lt;&gt; 0)
	left join itembal ib on (ib.ibitem=id.iditem and ibco=idco and ibloc=idloc)
	
where idohbl &lt;&gt; 0	
	and (iddate + (imshelflf - 60)days ) &lt;= current_date
	

	
')

order by company asc,location asc,expirationdate asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
