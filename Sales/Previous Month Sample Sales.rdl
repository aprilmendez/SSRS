  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select * from openquery(gsfl2k,'

SELECT 
 SHLINE.SLCO AS Company,
 BillTo.CMNAME AS BillToName,
 SHLINE.SLDESC as ItemDesc,
 sum(SHLINE.SLBLUS) as BillableUnitsShipped,
 sum(SHLINE.SLECST) as ExtendedCost

FROM SHLINE 
INNER JOIN CUSTMAST ON SHLINE.SLCUST = CUSTMAST.CMCUST 
INNER JOIN SHHEAD ON (SHLINE.SLCO = SHHEAD.SHCO 
						AND SHLINE.SLLOC = SHHEAD.SHLOC 
						AND SHLINE.SLORD# = SHHEAD.SHORD# 
						AND SHLINE.SLREL# = SHHEAD.SHREL# 
						AND SHLINE.SLINV# = SHHEAD.SHINV#)
LEFT JOIN ITEMMAST ON SHLINE.SLITEM = ITEMMAST.IMITEM 
INNER JOIN CUSTMAST BillTo ON SHHEAD.SHBIL# = BillTo.CMCUST

WHERE Year(SHHEAD.SHIDAT) = Year(current_date)
and month(shhead.shidat) = month(current_date) - 1

AND ITEMMAST.IMFCRG = ''S''  

Group by  

 SHLINE.SLCO,
 BillTo.CMNAME,
 SHLINE.SLDESC
')</CommandText>
      </Query>
      <Fields>
        <Field Name="COMPANY">
          <DataField>COMPANY</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BILLTONAME">
          <DataField>BILLTONAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ITEMDESC">
          <DataField>ITEMDESC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BILLABLEUNITSSHIPPED">
