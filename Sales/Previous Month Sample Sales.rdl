          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="EXTENDEDCOST">
          <DataField>EXTENDEDCOST</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
