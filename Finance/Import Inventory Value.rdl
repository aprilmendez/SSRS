  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select * from openquery(gsfl2k,'
select vmname,isitem, ISQOH, isvalu, IMFACT,


case 
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = '' '') then (IsQOH*itemmast.IMFACT)*imrcst
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = ''D'') then ((IsQOH*itemmast.IMFACT)/itemmast.IMFAC2)*imrcst
	else 0
end as NetValue


from itemstat
join itemmast on isitem = imitem
join vendmast on imvend = vmvend

where IMclas = ''IM''
and isloc = 98
and isqoh &lt;&gt; 0

order by vmname,imitem
')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
