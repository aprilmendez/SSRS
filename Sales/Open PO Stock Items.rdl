    <DataSource Name="GartmanReports">
      <DataSourceReference>/GartmanReports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>a89f32f6-9a4d-4479-a812-49c2bb77299f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select sku,
[description],
color,
plco,
loc,
PO,
--PLCO + '-' + LOC + '-' + PO as PurchaseOrder,
CONVERT(varchar(10),convert(datetime,duedate),101) as 'Due date',
--CONVERT(varchar(10),convert(datetime,ActualArrivalDueDate),101) as ExpectedArrival,
UnitsOrder,
InventoryUOM,
SalesQtyOrdered,
SalesUOM


from openquery(GSFL2K,'

Select 
case 
	when PLDDAT = ''0001-01-01'' then ''2999-12-31''
	else PLDDAT 
end as DueDate, 


case 
	when PLDDAT = ''0001-01-01'' then ''2999-12-31''
	else PLDDAT - 7 DAYS
end as ActualArrivalDueDate, 


Poline.PLCO, 
Poline.PLLOC As Loc, 
Vendmast.VMNAME As VendorName, 
Poline.PLPO# As PO, 
Poline.PLITEM As SKU, 
Poline.PLDESC As Description, 
Itemmast.IMCOLR As Color, 
Poline.PLQORD As UnitsOrder,
itemmast.imrpt2,
itemmast.imum1 as InventoryUOM,
case 
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = '' '') then (plqord*itemmast.IMFACT)
	when (itemmast.IMMD = ''M'' and itemmast.IMMD2 = ''D'') then ((plqord*itemmast.IMFACT)/itemmast.IMFAC2)
	else 0
end as SalesQtyOrdered,
itemmast.imum2 as SalesUOM


FROM Poline

left join Itemmast on Itemmast.IMITEM = Poline.PLITEM
Left Join Pohead On (Poline.PLco = Pohead.PHco
	and Poline.PLloc = Pohead.PHloc
	and poline.plvend = pohead.phvend
	and Poline.PLPO# = Pohead.PHPO#) 
Left Join Vendmast On Poline.PLVEND = Vendmast.VMVEND 


Where itemmast.imrpt2 = ''213''
AND Pohead.PHDOI &gt; ''12/31/2005''
and pldelt &lt;&gt; ''C''

Order By Poline.PLDDAT, Vendmast.VMNAME, Poline.PLPO#, Poline.PLITEM 
')
order by CONVERT(varchar(10),convert(datetime,ActualArrivalDueDate),101)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
