<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Inventory over 90 days old - link to customer sales since 1/1/2010</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="GartmanReports">
      <DataSourceReference>/GartmanReports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>29d8f4cf-55f1-4ad8-8c5e-972f2b30cf7f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Age">
            <Value>=Parameters!Age.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OverValue">
            <Value>=Parameters!OverValue.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  OldInventory.IDCO
  ,OldInventory.IDLOC
  ,OldInventory.Vendor
  ,OldInventory.iditem
  ,OldInventory.imdiv
  ,OldInventory.imfmcd
  ,OldInventory.IMCLS#
  ,OldInventory.imdesc
  ,OldInventory.imsi
  ,OldInventory.dropdate
  ,Sum(OldInventory.[value]) as InvValue
  ,Sum(OldInventory.[QtyAvailable]) as InvQty
FROM
  OldInventory

where ReceivingDate &lt; DATEADD(d,(@Age*-1),getdate())
and idco = @Company
and idloc = @Location

group by 
   OldInventory.IDCO
  ,OldInventory.IDLOC
  ,OldInventory.vendor
  ,OldInventory.iditem
  ,OldInventory.imdiv
  ,OldInventory.imfmcd
  ,OldInventory.IMCLS#
  ,OldInventory.imdesc
  ,OldInventory.imsi
  ,OldInventory.dropdate

having Sum(OldInventory.[value]) &gt; @OverValue</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
