  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>SELECT irloc 'Location',
	irreason 'Adj Code',
	iardes 'Reason',
	irdate 'Date',
	iritem 'item',
	irdesc 'Description',
	irbin 'Bin',
	ircomt 'Comments',
	irqty 'QTY',
	ircost 'Cost',
	iruser 'User',
	CAST((irqty * ircost)AS DECIMAL(10,3)) 'Total Cost' 	

FROM OPENQUERY (GSFL2K, '
SELECT irloc,irreason,irdate,iritem,irdesc,irbin,ircomt,irqty,ircost,iruser,iardes

FROM itemrech
left join iareason on irreason=iareas

where irdate = (CURRENT_DATE - 1 DAY)
and irloc = 85
')

ORDER BY irloc, irreason ASC ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
