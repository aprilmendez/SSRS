  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>/*  Payment on Accts - POA Report */

select * from openquery(gsfl2k,'
select o.oicust as Acct,
c.cmname as Customer,
o.oidate as EntryDate,
o.oiinv# as InvoiceNum,

oiamt as GrossAmt,
oinet as NetAmt

from openitem o
join custmast c on oicust = cmcust

where oiinv# like ''OA%''

and OICode = ''2''

order by oicust,oiinv#,oitype desc


')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
