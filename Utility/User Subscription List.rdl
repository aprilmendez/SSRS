<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Find reports a user is subscribed to</Description>
  <Author>Thomas Van Nuys</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>/GartmanReports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2a3839fc-4635-4ecb-ae51-830ddc07d53a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="parLogin">
            <Value>=Parameters!parLogin.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>="SELECT C.Name, S.LastRunTime FROM ReportServer..Subscriptions AS S LEFT OUTER JOIN ReportServer..[Catalog] AS C ON C.ItemID = S.Report_OID " &amp;  "left join ReportServer..reportschedule as Sched on Sched.subscriptionid = s.subscriptionid " &amp; "WHERE s.ExtensionSettings like '%" &amp;  Parameters!parLogin.Value &amp; "%'"</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LastRunTime">
          <DataField>LastRunTime</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
