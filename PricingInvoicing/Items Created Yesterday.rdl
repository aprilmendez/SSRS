  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select * from openquery (gsfl2k,'

select	imitem as ItemNum,
	imdesc as ItemDesc,
	imsdat as SetupDate,
	imdlm as LastMaintDate,
	imuser as LastMaintUser
from itemmast im
	left join itemxtra ix on ix.imxitm = im.imitem

where imsdat = (current_date)-1 day

')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
