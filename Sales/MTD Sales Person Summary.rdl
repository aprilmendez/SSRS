<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Summary report of Sales by Salesmen MTD.  Primarily for Danny Camp</Description>
  <Author>Thomas Van Nuys</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="GartmanReports">
      <DataSourceReference>/Data Sources/GartmanReports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e103edc9-0e51-4faa-95ac-84c92770c5c9</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select SalesPerson,
Year,
case Sales
	when null then 0
	else Sales
end as SalesDollars


from openquery(gsfl2k,'
select smname as SalesPerson,
Year(shidat) as Year,
sum(sleprc) as Sales

from shline
left join shhead on (shhead.shco = shline.slco
	and shhead.shloc = shline.slloc
	and shhead.shord# = shline.slord#
	and shhead.shrel# = shline.slrel#
	and shhead.shinv# = shline.slinv#
	and shhead.shcust = shline.slcust)
left join salesman on shline.SLSLMN = salesman.smno

where (
	(shidat &lt;= (current_date - 3 days) - 1 year 
	and year(shidat) = year(current_date) - 1
	and month(shidat) = month(current_date)) 
or 	(shidat &lt;= current_date 
	and year(shidat) = year(current_date)
	and month(shidat) = month(current_date))
	)

and shhead.shco = 1

group by smname,year(shidat)

order by smname,year(shidat)
')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
