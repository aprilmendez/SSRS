  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <CommandText>select * from openquery(gsfl2k,'
select smname as SalesPerson,
dvdesc as Division,
Year(shidat) as Year,
sum(sleprc) as Sales

from shline
left join shhead on (shhead.shco = shline.slco
	and shhead.shloc = shline.slloc
	and shhead.shord# = shline.slord#
	and shhead.shrel# = shline.slrel#
	and shhead.shinv# = shline.slinv#
	and shhead.shcust = shline.slcust)
left join salesman on shline.SLSLMN = salesman.smno
LEFT JOIN DIVISION ON SHLINE.SLDIV = DIVISION.DVDIV 

where (
	(shidat &lt;= (current_date - 3 days) - 1 year 
	and year(shidat) = year(current_date) - 1
	and month(shidat) = month(current_date)) 
or (shidat &lt;= current_date 
	and year(shidat) = year(current_date)
	and month(shidat) = month(current_date))
	)

and shco = 1

group by smname,dvdesc,year(shidat)

order by smname,year(shidat),dvdesc
')</CommandText>
        <rd:DesignerState><QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational"><SelectedColumns><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="InvoiceDate" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="Company" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="SalesName" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="BillToCustID" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="BillToCustName" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="BillToCity" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="BillToState" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="VendorNum" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="VendorName" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="FamilyCode" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="FamilyCodeDesc" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="Division" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="DivisionDesc" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="SalesDivision" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="Price" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="Cost" /><ColumnExpression ColumnOwner="CustomerCurrentMonthSales" ColumnName="Profit" /></SelectedColumns></QueryDefinition></rd:DesignerState>
      </Query>
