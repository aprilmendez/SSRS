      <DataSourceReference>/Data Sources/GartmanReports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4a5c258c-6148-42d0-8409-3d2eb25d261c</rd:DataSourceID>
