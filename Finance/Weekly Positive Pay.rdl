  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@qStartDate">
            <Value>=FormatDateTime(Parameters!pStartDate.Value,DateFormat.ShortDate)</Value>
          </QueryParameter>
          <QueryParameter Name="@qEndDate">
            <Value>=FormatDateTime(Parameters!qEndDate.Value,DateFormat.ShortDate)</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @sql varchar(max)

set @sql = 'select CheckNum,
CheckAmt,
CONVERT(datetime, CONVERT(VARCHAR(10), ACHCHECKDT)) as CheckDate,
Payee 
from openquery(gsfl2k,''
SELECT ACHCHECK# AS CheckNum, 
ACHCKAMT AS CheckAmt, 
ACHCHECKDT, 
APCHECKHST.ACHVNM AS Payee

FROM APCHECKHST
WHERE APCHECKHST.ACHCHECKDT Between ''''' + @qStartDate + ''''' And ''''' + @qEndDate + ''''' 
and ACHCKAMT &gt; 0
and ACHCHECK# &lt;&gt; 0
and not (ACHCKAMT = .01 and APCHECKHST.ACHVNM = ''''MANNINGTON MILLS INC'''')
ORDER BY APCHECKHST.ACHCHECK#
'')'

exec(@sql)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
