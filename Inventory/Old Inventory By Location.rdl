    <DataSet Name="LocationList">
      <Query>
        <DataSourceName>GartmanReports</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select lcco,
lcloc,
lcname,
lcrnam
from gsfl2k.b107fd6e.gsfl2k.location
where lcname not like '~%'
and lcco = @Company
and lctrcs &lt;&gt; ' '</CommandText>
      </Query>
